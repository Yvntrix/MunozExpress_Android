                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="-30dp"
                android:gravity="center"
                android:text="Pakuha Details"
                android:textSize="20sp"
                android:textStyle="bold" />
        </LinearLayout>
